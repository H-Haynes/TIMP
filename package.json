{
  "name": "timp-music",
  "version": "2.0.0",
  "description": "TIMP音乐播放器",
  "author": "H-Haynes<3066620060@qq.com>",
  "license": "MIT",
  "private": true,
  "main": "dist-electron/main/index.js",
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue",
    "TIMP音乐"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "app:dev": "vite",
    "app:build": "vite build && electron-builder",
    "app:preview": "vite preview",
    "lint": "eslint --ext .js,.vue,.ts --ignore-path .gitignore --fix src",
    "format": "prettier .  --write",
    "deps:update": "taze major -I",
    "compile": "npm run app:build && electron-builder build --config .electron-builder.config.js --dir --config.asar=false",
    "dist": "npm run app:build & electron-builder"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.2",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vueuse/core": "^10.7.1",
    "autoprefixer": "^10.4.16",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-vue": "^9.8.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persist": "^1.0.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "sass": "^1.69.7",
    "tailwindcss": "^3.4.0",
    "taze": "^0.13.1",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-element-plus": "^0.8.0",
    "vite": "^5.0.10",
    "vite-plugin-electron": "^0.15.5",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.4.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.0.22"
  },
  "dependencies": {
    "@vue/devtools": "^7.0.20",
    "axios": "^1.6.5",
    "electron-devtools-installer": "^3.2.0",
    "element-plus": "^2.4.4",
    "mitt": "^3.0.1",
    "normalize.css": "^8.0.1",
    "pinia-auto-refs": "^0.0.10",
    "qs": "^6.11.2",
    "throttle-debounce": "^5.0.0",
    "unocss": "^0.58.3",
    "unplugin-vue-components": "^0.26.0"
  }
}